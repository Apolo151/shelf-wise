{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "docs/swagger.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/docs",
      "dest": "/docs/swagger.json"
    },
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ]
}
